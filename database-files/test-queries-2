SELECT
    C."COUNTRY_NAME"
    , SOP."STATE_NAME"
    , SOP."LATITUDE"
    , SOP."LONGITUDE"
    , RR."NUM_RECOVERED"
--    , RD."NUM_DEATHS"
    , RC."NUM_CASES"
--    , RA."NUM_ACTIVE"
--    , HB."BEDS_PER_1K"
--    , HB."TOTAL_BEDS"
FROM test."STATE_OR_PROVINCE" SOP
join test."COUNTRIES" C on C."COUNTRY_ID"=SOP."COUNTRY_ID"
join test."REPORTED_RECOVERED" RR on RR."STATE_ID"=SOP."STATE_ID"
-- join test."REPORTED_DEATHS" RD on RD."STATE_ID"=SOP."STATE_ID"
 join test."REPORTED_CASES" RC on RC."STATE_ID"=SOP."STATE_ID"
 join test."REPORTED_ACTIVE" RA on RA."STATE_ID"=SOP."STATE_ID"
 join test."HOSPITAL_BEDS" HB on HB."STATE_ID"=SOP."STATE_ID";
